	keywords = {child, China, controlled study, education, educational status, female, Female, Food Services, food waste, Food waste, health education, human, human experiment, Humans, Influence factor, major clinical study, meal, Meals, middle school student, nutrition policy, Nutrition policy, primary school, questionnaire, randomized controlled trial, read\_KA, Refuse Disposal, resident, school, School meals, Schools, staple food, student, Students, vegetable, waste disposal},
